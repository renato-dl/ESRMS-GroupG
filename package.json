{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "test": "jest --runInBand",
    "server": "babel-node ./src",
    "server:prod": "node ./dist",
    "dev": "cross-env NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir ./dist",
    "prod": "cross-env NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "^1.7.3",
    "http-status": "^1.4.0",
    "jest": "^24.9.0",
    "mariadb": "^2.1.2",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "nodemon": "^1.19.4"
  },
  "env": {
    "test": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs"
      ]
    }
  }
}
